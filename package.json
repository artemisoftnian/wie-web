{
  "name": "wiecare-web-app",
  "version": "1.0.0",
  "repository": "wiecare/wie-web",
  "homepage": "https://www.wiecare.org",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "babel-preset-gatsby": "0.11.0",
    "dotenv": "8.2.0",
    "gatsby": "2.31.1",
    "gatsby-background-image": "1.4.0",
    "gatsby-image": "2.10.0",
    "gatsby-link": "2.10.0",
    "gatsby-plugin-create-client-paths": "2.9.0",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-fontawesome-css": "^1.0.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-mailchimp": "5.2.2",
    "gatsby-plugin-netlify-cms": "^4.9.0",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.9.0",
    "gatsby-plugin-sharp": "^2.13.4",
    "gatsby-source-filesystem": "2.10.0",
    "gatsby-transformer-sharp": "2.11.0",
    "generate": "0.14.0",
    "imagemin-pngquant": "^5.0.1",
    "install": "^0.13.0",
    "intersection-observer": "0.12.0",
    "netlify-cms-app": "2.14.14",
    "npm": "^6.14.11",
    "nuka-carousel": "4.7.5",
    "pngquant-bin": "^3.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-hook-form": "5.7.2",
    "react-icons": "3.10.0",
    "react-modal": "3.11.2",
    "react-slick": "0.26.1",
    "react-toastify": "6.0.5",
    "react-typed": "^1.2.0",
    "reading-time": "1.2.0",
    "sharp": "0.27.1",
    "slick-carousel": "1.8.1",
    "typeface-lato": "0.0.75",
    "typeface-roboto-slab": "1.1.4",
    "typeface-source-sans-pro": "1.1.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "enable:blog": "node ./scripts/enable-blog-modules.js",
    "disable:blog": "node -e 'require(\"./scripts/disable-blog-modules.js\").resetConfig()'",
    "format": "npm run format:src && npm run format:test && yarn run format:templates",
    "format:src": "prettier --write \"src/**/*.js\"",
    "format:templates": "prettier --write \"templates/**/*.js\"",
    "format:test": "prettier --write \"tests/**/*.js\"",
    "heroku-postbuild": "gatsby build",
    "lint": "lint-staged",
    "test": "jest",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:watch": "jest --watch",
    "view:coverage": "open coverage/lcov-report/index.html",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -b master",
    "github": "gatsby build --prefix-paths",
    "dep": "gh-pages -d public -r https://$GH_TOKEN@github.com/artemisoftnian/wie-web.git"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@testing-library/jest-dom": "5.8.0",
    "@testing-library/react": "10.0.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-preset-gatsby-package": "0.4.2",
    "cross-spawn-with-kill": "1.0.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "gatsby-plugin-purgecss": "5.0.0",
    "gh-pages": "^3.1.0",
    "jest": "26.0.1",
    "lint-staged": "10.2.4",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "yarn format"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup-test-env.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/tests/jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "./tests/jest-preprocess.js",
      "./tests/loadershim.js"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "!/static/vendor/jquery/dist"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/image-mock.js",
      "\\.(css|scss|sass)$": "<rootDir>/tests/__mocks__/style-mock.js"
    }
  },
  "resolutions": {
    "sharp": "0.25.3"
  }
}
